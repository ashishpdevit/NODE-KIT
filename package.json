{
    "name": "node-starter-kit",
    "version": "1.0.0",
    "description": "A minimal TypeScript starter kit for building Express APIs.",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon --watch src --ext ts --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
        "build": "tsc -p tsconfig.build.json",
        "start": "node dist/index.js",
        "test": "jest",
        "prisma:generate": "prisma generate",
        "db:seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
        "prisma:deploy": "prisma migrate deploy",
        "prisma:migrate": "prisma migrate dev",
        "prisma:studio": "prisma studio"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.906.0",
        "@azure/storage-blob": "^12.28.0",
        "@prisma/client": "^5.17.0",
        "@types/bull": "^3.15.9",
        "@types/handlebars": "^4.0.40",
        "@types/mime-types": "^3.0.1",
        "@types/multer": "^2.0.0",
        "@types/uuid": "^10.0.0",
        "aws-sdk": "^2.1692.0",
        "bcryptjs": "^2.4.3",
        "bull": "^4.16.5",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.19.2",
        "firebase-admin": "^13.5.0",
        "handlebars": "^4.7.8",
        "helmet": "^7.1.0",
        "ioredis": "^5.8.1",
        "jsonwebtoken": "^9.0.2",
        "mime-types": "^3.0.1",
        "multer": "^2.0.2",
        "nodemailer": "^7.0.6",
        "twilio": "^5.10.2",
        "uuid": "^13.0.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.12.12",
        "@types/nodemailer": "^7.0.2",
        "@types/supertest": "^2.0.12",
        "jest": "^29.7.0",
        "nodemon": "^3.1.0",
        "prisma": "^5.17.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
